{
  "name": "algorithm-blog",
  "version": "1.0.0",
  "description": "硬核C++算法题解 | 数据结构深度解析 | 竞赛编程实战",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs --port 3000 --host 0.0.0.0",
    "docs:build": "vitepress build docs && touch docs/.vitepress/dist/.nojekyll",
    "docs:preview": "vitepress preview docs --port 4000 --host 0.0.0.0",
    "docs:serve": "npm run docs:build && npm run docs:preview",
    "deploy": "bash deploy.sh",
    "setup": "npm install && npm run docs:build"
  },
  "keywords": [
    "算法",
    "C++",
    "线段树",
    "动态规划",
    "图论",
    "竞赛编程"
  ],
  "author": "Augustus_Deception",
  "license": "MIT",
  "devDependencies": {
    "vitepress": "^1.6.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
